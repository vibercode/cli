# VibeCode CLI Environment Variables
# Copy this file to .env and fill in your actual values

# Claude AI API Configuration
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-api03-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Preview Server Configuration
VIBE_PREVIEW_PORT=3001
VIBE_PREVIEW_HOST=localhost

# Database Configuration (for generated projects)
DATABASE_URL=mongodb://localhost:27017/vibercode
POSTGRES_URL=postgres://user:password@localhost:5432/vibercode
MYSQL_URL=mysql://user:password@localhost:3306/vibercode

# Supabase Configuration (for generated projects)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_SERVICE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Development Settings
DEBUG=true
LOG_LEVEL=info

# Template Repository (for updates)
TEMPLATE_REPO_URL=https://github.com/vibercode/templates
TEMPLATE_BRANCH=main

# Generated Project Defaults
DEFAULT_MODULE_PREFIX=github.com/yourorg
DEFAULT_PORT=8080
DEFAULT_DATABASE=mongodb